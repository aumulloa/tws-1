program pr3c12:
#
#	outputs 1 2 3 4 5 on separate lines
#
const sunday    = 0;
      monday    = 1;
      tuesday   = 2;
      wednesday = 3;
      thursday  = 4;
      friday    = 5;
      saturday  = 6;
var
	i : integer;
begin
	for i := monday to friday do output(ord(i)-ord(sunday));
end pr3c12.
